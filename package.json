{
  "name": "vite-vue-ts",
  "version": "0.0.0",
  "description": "A Vite2 + Typescript + Vue3 + Element-ui + Scss + Eslint + Prettier template",
  "homepage": "https://github.com/lotosv2010/vite-vue3-ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:lotosv2010/vite-vue3-ts.git"
  },
  "keywords": [
    "vite",
    "typescript",
    "vue",
    "element-plus",
    "scss",
    "eslint",
    "prettier"
  ],
  "author": {
    "name": "Robin",
    "email": "lotosv2010@163.com",
    "github": "https://github.com/lotosv2010"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vite build --mode test",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write .",
    "stylelint": "stylelint --fix \"**/*.{scss,vue,css}\"",
    "prepare": "husky install",
    "test": "pnpm unit && pnpm e2e-run",
    "unit": "jest --colors --passWithNoTests",
    "unit-watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "e2e": "cypress open",
    "e2e-run": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,scss}": "npm run stylelint",
    "*.{js,jsx,vue,ts,tsx}": "npm run lint",
    "*.{js,jsx,vue,tsx,ts,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "axios": "^0.26.1",
    "element-plus": "^2.1.10",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.5.1",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "mrm": "^4.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.50.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-checker": "^0.4.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.29.8"
  }
}
